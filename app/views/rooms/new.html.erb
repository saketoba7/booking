

<%= form_for @room do |f| %>
    <div>
        <%= f.label :room_name%><br>
        <%= f.text_field :room_name %>
    </div>
    <div>
        <%= f.label :room_price %><br>
        <%= f.text_field :room_price %>
    </div>
    <div>
        <%= f.label :room_address %><br>
        <%= f.text_area :room_address %>
    </div>
    <div>
        <%= f.label :room_content %><br>
        <%= f.text_area :room_content %>
    </div>
    <div>
        <%= f.label :image %><br/>
        <%= f.file_field :image %>
    </div>
    <div><%= f.submit "投稿する" %></div>
<% end %>


